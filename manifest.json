{
    "manifest_version": 3,
    "name": "EstateEvaluator",
    "version": "1.0",
    "description": "Enhance your browsing experience on real estate pages by providing insightful evaluations.",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.finn.no/realestate/homes/*"],
        "js": ["scripts/contentScript.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  